{
  "name": "Study Sprite",
  "description": "Academic Tracker",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "dashboard.html",
    "default_icon": "/closet/study_sprite_logo.png"
  },
  "icons": {
    "16": "/closet/study_sprite_logo.png",
    "48": "/closet/study_sprite_logo.png",
    "128": "/closet/study_sprite_logo.png" 
  },
  "background": {
    "service_worker": "background.js"

  },
  "web_accessible_resources": [
    {
      "resources": ["circle-progress.min.js"],
      "matches": ["<all_urls>"]
    }
  ]

}